<!-- USER DATA-->
<div class="user-data m-b-30">
  <h3 class="title-3 m-b-30">
    <i class="zmdi zmdi-account-calendar"></i><%__('customers')%></h3>
  <div class="table-responsive table-data">
    <table class="table">
      <thead>
      <tr>
        <td>
          <label class="au-checkbox">
            <input type="checkbox">
            <span class="au-checkmark"></span>
          </label>
        </td>
        <td><%=__('Name')%></td>
        <td><%=__('Email')%></td>
        <td><%=__('Balance')%></td>
        <td><%=__('Date')%></td>
        <td></td>
      </tr>
      </thead>
      <tbody>
      <% customers.forEach(function(customer) { %>
      <tr>
        <td>
          <label class="au-checkbox">
            <input type="checkbox">
            <span class="au-checkmark"></span>
          </label>
        </td>
        <td>
          <div class="table-data__info">
            <h6><%=customer.user.name%></h6>
          </div>
        </td>
        <td>
          <span class="table-data__info"><%=customer.user.username%></span>
        </td>
        <td>
          <span class="role admin">$<%=customer.balance%></span>
        </td>
        <td>
          <span class="table-data__info"><%=customer.createdAt%></span>
        </td>
        <td>
          <div class="table-data-feature">
            <button class="item" data-toggle="tooltip" data-placement="top" title="<%=__('Ban')%>">
              <i class="zmdi zmdi-close-circle-o"></i>
            </button>
              <button class="item" data-toggle="tooltip" data-placement="top" title="<%=__('Delete')%>">
                <i class="zmdi zmdi-delete"></i>
              </button>
          </div>
        </td>
      </tr>
      <% }) %>
      </tbody>
    </table>
  </div>
  <div class="user-data__footer">
    <button class="au-btn au-btn-load">load more</button>
  </div>
</div>
<!-- END USER DATA-->
