<!--                        dreamcatcher synth. Cosby sweater eu banh mi, irure terry richardson ex sd. Alip placeat salvia cillum iphone.-->
<!--                        Seitan alip s cardigan american apparel, butcher voluptate nisi .</p>-->

                      <div class="form-check">
                        <% data.offers[gateway.id].forEach(function(offer) { %>
                        <div class="radio">
                          <label for="radio<%=offer.id%>" class="form-check-label ">
                            <input
                                    type="radio"
                                    id="radio<%=offer.id%>"
                                    name="radios"
                                    value="option<%=offer.id%>"
                                    class="form-check-input">
                            <%=offer.amount%> <%=offer.domain.currency.symbol%>
                            <%if(offer.bonus && offer.bonus>0){%>
                            (<%=offer.bonus%> <%=offer.domain.currency.symbol%> offer)
                            <%}%>
                          </label>
                        </div>
                          <% }); %>

                      </div>
                    </div>
                  <% }); %>

