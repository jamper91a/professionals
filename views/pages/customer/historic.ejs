

<section class="p-t-20">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h3 class="title-5 m-b-35"><%=__('Chats')%></h3>
                <div class="table-responsive table-responsive-data2 m-b-30">
                    <table class="table table-borderless table-striped table-earning">
                        <thead>
                        <tr>
                            <th><%=__('Date')%></th>
                            <th><%=__('Duration')%></th>
                            <th><%=__('Professional')%></th>
                            <th><%=__('Messages')%></th>
                        </tr>
                        </thead>
                        <tbody>
                        <%
                        if(historic  && historic.chats && historic.chats.length>0) {
                            historic.chats.forEach(function (chat) {
                        %>
                            <tr>
                                <td><%=chat.createdAt%></td>
                                <td><%=chat.duration%></td>
                                <td><%=chat.professional.name%></td>
                                <td><a href="#"><%=__('Messages')%></a> </td>
                            </tr>
                        <%
                            })
                            }else{
                          %>
                            <tr><td colspan="4">no ada</td></tr>
                        <%
                            }
                        %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
